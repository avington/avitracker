<!DOCTYPE html>
<html lang="en"  ng-app="ngTracker">
  <head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="AviTracker, The developers way to track time">
    <meta name="author" content="Steven Moseley">

    <!-- Le styles -->
    @Styles.Render("~/bundles/cerulean/css")
    @Styles.Render("~/Content/themes/base/css")
    <style>
      body {
        padding-top: 60px; /* 60px to make the container go all the way to the bottom of the topbar */
      }
    </style>
    @Styles.Render("/bundles/responsive")

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="~/Scripts/html5shiv.js"></script>
    <![endif]-->
    @* favicons and touch icons go here *@
      @Scripts.Render("~/bundles/jquery")
  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="#">Project name</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li class="active"><a href="#">Home</a></li>
              <li><a href="#about">Accounts Receivable</a></li>
              <li><a href="#contact">Contact</a></li>
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">
        @RenderBody()
    </div> <!-- /container -->
     @Scripts.Render("~/bundles/jqueryui")
     @Scripts.Render("~/bundles/lib")

  </body>
</html>